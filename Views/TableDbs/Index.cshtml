@model IEnumerable<Book_My_Table.Models.TableDb>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RestaurantId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RestaurantId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TableDbId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TableDbId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TableDbId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
